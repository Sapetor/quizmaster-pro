{
  "name": "quizmaster-pro",
  "version": "1.0.0",
  "description": "QuizMaster Pro - Advanced interactive quiz platform for local networks",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build:css": "postcss public/css/main.css -o public/css/main.min.css --verbose",
